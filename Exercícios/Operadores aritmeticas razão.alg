algoritmo "soma"
// Função :
// Autor :
// Data : 05/01/2022
// Seção de Declarações 
var N1, N2: Inteiro
    M: Real

inicio
  Escreva (" Informe um numero: ")
  Leia (N1)
  Escreva (" Informe um numero: ")
  Leia (N2)
  M <- (N1+N2)/2
  Escreva ( "A média entre ", N1, " e", N2, " é igual a ", M)
  
fimalgoritmo
