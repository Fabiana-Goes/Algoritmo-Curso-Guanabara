algoritmo "creuza04"
// Função :
// Autor :
// Data : 10/01/2022
// Seção de Declarações 
var
   Produto, Imposto : real

inicio
      Escreva ( " Qual o valor do produto US$? ")
      Leia (Produto)
      Imposto <- (produto*60)/100
      Escreva ( " Valor do imposto US$", imposto:5:2)
fimalgoritmo
