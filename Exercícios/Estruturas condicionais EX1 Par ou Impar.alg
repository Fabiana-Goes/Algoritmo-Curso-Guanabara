algoritmo "PArOUImpar"
// Função :
// Autor :
// Data : 10/01/2022
// Seção de Declarações 
var
   N: Inteiro

inicio
      Escreva ( "Digite um numero qualquer ")
      Leia (N)
      Se (N%2 = 0) entao
         EscrevaL ( "O numero", N, " é par.")
      Senao
         EscrevaL ( "O Numero", N, " é impar.")
      FimSe
fimalgoritmo
