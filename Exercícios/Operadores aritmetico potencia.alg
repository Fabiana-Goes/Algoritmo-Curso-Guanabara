algoritmo "semnome"
// Função :
// Autor :
// Data : 05/01/2022
// Seção de Declarações 
var
  N1, R1, E1: real

inicio

  Escreva (" Informe um numero: ")
  Leia (N1)
  Escreva (" Informe uma potencia: ")
  Leia (R1)
  E1 <- Exp(N1,R1)
  Escreva (N1, " elevado a potencia ", R1, " é igual a ", E1)
  
fimalgoritmo
